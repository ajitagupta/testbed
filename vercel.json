{
    "version": 2,
    "builds": [
      { "src": "*.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/", "dest": "/index.html" },
      { "src": "/locations", "dest": "/locations.html" },
      { "src": "/faq", "dest": "/faq.html" },
      { "src": "/contact", "dest": "/contact.html" },
      { "src": "/(.+)", "dest": "/$1" }
    ]
  }